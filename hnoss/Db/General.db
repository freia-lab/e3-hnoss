record (calc, "CstatH-Ctrl:Alarms:sGeneral") {
  field(SCAN, "1 second")
  field(INPA, "CstatH-Ctrl:Alarms:sAlarmB1")
  field(INPB, "CstatH-Ctrl:Alarms:sAlarmB10")
  field(INPC, "0x1F")
  field(INPD, "0xFC")
  field(PREC, "10")
  field(CALC, "(A&C)+((B&D)<<8)")
}

