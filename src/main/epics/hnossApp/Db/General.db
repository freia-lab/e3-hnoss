  field(INPC, "CstatH-Ctrl:Alarms:sAlarmB6")
  field(INPD, "CstatH-Ctrl:Alarms:sAlarmB7")
  field(INPE, "0x07")
  field(PREC, "10")
  field(CALC, "(A>>4)+(B<<4)+(C<<12)+((D&E)<<20)")
}
record (calc, "CstatH-Ctrl:Alarms:sMKS3") {
  field(SCAN, "1 second")
