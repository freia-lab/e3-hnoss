
record (bi, "CstatH-Ctrl:s7-status") {
  field (DTYP, "S7plc stat")
  field (INP,  "@PLC:HNOSS")
  field (SCAN, "I/O Intr")
  field (ZNAM, "disconnected")
  field (ONAM, "connected")
  field (ZSV,  "MAJOR")
  field (FLNK, "CstatH-Ctrl:s7-status-counter")
}

